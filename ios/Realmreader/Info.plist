    <!-- Access photo library (read) -->
    <key>NSPhotoLibraryUsageDescription</key>
    <string>This app needs access to your photo library to select files or images.</string>

    <!-- Add to photo library (write) -->
    <key>NSPhotoLibraryAddUsageDescription</key>
    <string>This app needs permission to save files or images to your photo library.</string>

    <!-- Camera (optional, if you ever add camera functionality) -->
    <key>NSCameraUsageDescription</key>
    <string>This app needs access to the camera to take photos.</string>

    <!-- Files/Documents support for document picker -->
    <key>LSSupportsOpeningDocumentsInPlace</key>
    <true/>

    <key>RCTNewArchEnabled</key>
    <true/>
